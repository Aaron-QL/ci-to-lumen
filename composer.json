{
    "name": "ofashion/cframe",
    "description": "A fucking awesome PHP framework born for Backend Artisans.",
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "^2.9",
        "illuminate/config": "^5.7",
        "illuminate/container": "^5.7",
        "illuminate/events": "^5.7",
        "illuminate/http": "^5.7",
        "illuminate/log": "^5.7",
        "illuminate/pipeline": "^5.7",
        "illuminate/validation": "^5.7",
        "nikic/fast-route": "^1.3",
        "symfony/psr-http-message-bridge": "^1.1",
        "vlucas/phpdotenv": "^3.1",
        "zendframework/zend-diactoros": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "akaQin",
            "email": "long.qin@mfashion.com.cn"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "OFashion\\CFrame\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
